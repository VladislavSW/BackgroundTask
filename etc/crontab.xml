<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="scandiweb_background_task">
        <job name="scandiweb_background_task_runner_1"
             instance="Scandiweb\BackgroundTask\Cron\BackgroundTaskRunner" method="execute">
            <config_path>system/background_task/task_runner_cron_expression</config_path>
        </job>
        <job name="scandiweb_background_task_runner_2"
             instance="Scandiweb\BackgroundTask\Cron\BackgroundTaskRunner" method="execute">
            <config_path>system/background_task/task_runner_cron_expression</config_path>
        </job>
        <job name="scandiweb_background_task_runner_3"
             instance="Scandiweb\BackgroundTask\Cron\BackgroundTaskRunner" method="execute">
